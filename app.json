{
    "name": "Sanila-Assistant-bot",
    "description": "Powerful Telegram bot to connect with sanila",
    "repository": "https://github.com/sanila2007/Sanila-Assistant-Bot",
    "logo": "https://telegra.ph/file/73550b9954e83d4a571fc.jpg",
    "keywords": [
        "countdown",
        "timer",
        "important events",
        "countdown timer",
        "Telegram bot",
        "Pyrogram bot"
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Get from Telegram's @BotFather",
            "required": true
        },
        "LOG_CHANNEL": {
            "description": "Create a public channel and enter the username here",
            "required": true
        },
        "FEEDBACK_CHANNEL": {
            "description": "Create a public channel and enter the username here (optional)",
            "required": false
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
